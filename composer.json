{
  "name": "zetta-code/zend-bootstrap",
  "type": "library",
  "description": "Zetta\\ZendBoostrap",
  "license": "MIT",
  "keywords": [
    "bootstrap",
    "zf3"
  ],
  "homepage": "https://github.com/zetta-code/zend-bootstrap",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "homepage": "http://thiagossantos.com.br",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zetta\\ZendBootstrap\\": "src/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-intl": "*",
    "zendframework/zend-stdlib": "^3.2"
  },
  "require-dev": {
    "nesbot/carbon": "^2.28",
    "zendframework/zend-captcha": "^2.9",
    "zendframework/zend-form": "^2.14",
    "zendframework/zend-mail": "^2.10",
    "zendframework/zend-mvc": "^3.1",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.2",
    "zendframework/zend-navigation": "^2.9",
    "zendframework/zend-paginator": "^2.8",
    "zendframework/zendservice-recaptcha": "^3.2",
    "doctrine/doctrine-orm-module": "^2.1"
  },
  "extra": {
    "zf": {
      "component": "Zetta\\ZendBootstrap",
      "config-provider": "Zetta\\ZendBootstrap\\ConfigProvider"
    }
  }
}
