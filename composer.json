{
  "name": "zetta-code/zend-bootstrap",
  "type": "library",
  "description": "ZendBootstrap | Laminas Bootstrap by ZettaCode",
  "license": "MIT",
  "keywords": [
    "bootstrap",
    "laminas"
  ],
  "homepage": "https://github.com/zetta-code/zend-bootstrap",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "homepage": "https://thi.dev.br",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zetta\\ZendBootstrap\\": "src/"
    }
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "laminas/laminas-stdlib": "^3.5"
  },
  "require-dev": {
    "doctrine/doctrine-orm-module": "^4.0",
    "laminas/laminas-captcha": "^2.10",
    "laminas/laminas-form": "^2.17",
    "laminas/laminas-mail": "^2.14",
    "laminas/laminas-mvc": "^3.2",
    "laminas/laminas-mvc-plugins": "^1.1",
    "laminas/laminas-navigation": "^2.11",
    "laminas/laminas-paginator": "^2.10",
    "laminas/laminas-recaptcha": "^3.3",
    "nesbot/carbon": "^2.51",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "extra": {
    "laminas": {
      "component": "Zetta\\ZendBootstrap",
      "config-provider": "Zetta\\ZendBootstrap\\ConfigProvider"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --coverage-clover build/clover.xml"
  }
}
